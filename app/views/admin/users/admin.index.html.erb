 <div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th>名前</th>
      <th>メールアドレス</th>
      <th>製作日</th>
      <th></th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |admin| %>
      <tr>
        <td><%= admin.user.id %></td>
        <td><%= admin.user_name %></td>
        <td><%= admin.created_at.strftime("%Y-%m-%d %H:%M") %></td>
        
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>